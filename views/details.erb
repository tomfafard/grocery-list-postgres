<h1>"<%=@grocery["name"]%>" details:</h1>

<h3>Comments:</h3>

<ul id="comment-list">
  <% @comments.each do |comment| %>
    <li class="comment">
      <%= comment["comment"] %>
    </li>
  <% end %>
</ul>

<form action="/groceries" method="get">
  <input type="submit" value="Back to List">
</form>
